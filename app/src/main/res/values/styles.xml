<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--分割线-->
    <style name="line_1">
        <item name="android:layout_width">@dimen/mm_1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/C_FFFFFF</item>
    </style>

    <!--分割线-->
    <style name="line_2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/mm_1</item>
        <item name="android:background">@color/C_FFFFFF</item>
    </style>

    <!--TextView-->
    <style name="textView_calculator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_calculator_bg</item>
        <item name="android:padding">@dimen/mm_18</item>
        <item name="android:textColor">@color/C_FFFFFF</item>
        <item name="android:textSize">@dimen/sp_38</item>
    </style>

</resources>
